{
  "name": "ngx-glide",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "cz": "git-cz",
    "prepare": "husky install",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "release": "semantic-release",
    "start:ngx-glide-demo": "nx serve ngx-glide-demo",
    "build:ngx-glide-demo": "nx build ngx-glide-demo --base-href=\"/ngx-glide/\"",
    "build:ngx-glide": "nx build ngx-glide && cp README.md dist/packages/ngx-glide && cp LICENSE dist/packages/ngx-glide",
    "build:all": "nx run-many --target=build --all --parallel --maxParallel=3",
    "affected:lint": "nx affected --target=lint --parallel --maxParallel=3",
    "affected:test": "nx affected --target=test --parallel --maxParallel=3",
    "affected:build": "nx affected --target=build --parallel --maxParallel=3",
    "current-version": "grep '\"version\"' package.json | cut -d '\"' -f 4 | head -n 1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.0",
    "@angular/cdk": "~14.0.0",
    "@angular/common": "~14.0.0",
    "@angular/compiler": "~14.0.0",
    "@angular/core": "~14.0.0",
    "@angular/forms": "~14.0.0",
    "@angular/material": "~14.0.0",
    "@angular/platform-browser": "~14.0.0",
    "@angular/platform-browser-dynamic": "~14.0.0",
    "@angular/router": "~14.0.0",
    "@glidejs/glide": "^3.5.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.0.0",
    "@angular-eslint/eslint-plugin": "~13.2.1",
    "@angular-eslint/eslint-plugin-template": "~13.2.1",
    "@angular-eslint/template-parser": "~13.2.1",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "~14.0.0",
    "@angular/language-service": "~14.0.0",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@nrwl/angular": "14.2.4",
    "@nrwl/cli": "14.2.4",
    "@nrwl/cypress": "14.2.4",
    "@nrwl/eslint-plugin-nx": "14.2.4",
    "@nrwl/jest": "14.2.4",
    "@nrwl/linter": "14.2.4",
    "@nrwl/workspace": "14.2.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
    "@typescript-eslint/parser": "~5.24.0",
    "commitizen": "^4.2.4",
    "cypress": "^9.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "lint-staged": "^13.0.1",
    "ng-packagr": "~14.0.0",
    "nx": "14.2.4",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-rational-order": "^1.0.3",
    "semantic-release": "^19.0.3",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
